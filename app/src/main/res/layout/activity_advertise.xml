<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <com.dog.manage.zhifa.app.view.TitleView
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_48"
            android:background="@color/colorAppTheme"
            app:tv_content=""
            app:tv_content_size="@dimen/sp_14"
            app:tv_title="禁养犬宣传"
            app:tv_title_size="@dimen/titleViewSize" />

        <com.dog.manage.zhifa.app.view.TextBoldView
            android:id="@+id/noticeTitleView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dp_16"
            android:gravity="center"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            tools:text="禁养犬宣传-北京市公安局" />

        <TextView
            android:id="@+id/noticeTimeView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#999999"
            android:textSize="@dimen/sp_12"
            android:textStyle="normal"
            tools:text="2021.08.09 " />

        <WebView
            android:id="@+id/webView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:overScrollMode="never"
            android:layout_margin="@dimen/dp_15" />

    </LinearLayout>
</layout>